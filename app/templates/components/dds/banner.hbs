<div class="container-fluid {{background}}">
  {{#if links}}
    {{dds/bread-crumbs links=links}}
  {{/if}}
  {{#if titleType}}
    <h3 class="au-banner__title {{titleType}}">{{title}}</h3>
  {{else}}
    <h3 class="au-banner__title">{{title}}</h3>
  {{/if}}
  {{#if heading}}
    <br>
    <h4 class="au-banner__heading">{{heading}}</h4>
  {{/if}}
  {{#if subline}}
    <div class="au-banner__subline">{{subline}}</div>
  {{/if}}
  {{yield}}
</div>
